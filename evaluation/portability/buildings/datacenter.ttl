@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix bldg: <https://example.org/datacenter#> .
@prefix ex:   <https://example.org/ontobot#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

# A simplified single-floor data center with zones and CRAC units
bldg:DataCenter a brick:Building .

bldg:Floor_DC a brick:Floor ; ex:floorNumber 1 .
 bldg:ColdAisle a brick:Zone .
 bldg:HotAisle  a brick:Zone .

# CRAC units on the only floor
bldg:CRAC1 a brick:Air_Handling_Unit ; brick:isLocatedIn bldg:Floor_DC .

# CO2 Sensors and Setpoints (not typical for DC, but included to prove portability of the same query set)

bldg:ColdAisle ex:hasCO2 bldg:ColdAisle_CO2 ; ex:setpoint bldg:ColdAisle_CO2_SP .
 bldg:ColdAisle_CO2 a brick:CO2_Sensor ; ex:value 600 .
 bldg:ColdAisle_CO2_SP a brick:CO2_Setpoint ; ex:value 700 .

bldg:HotAisle ex:hasCO2 bldg:HotAisle_CO2 ; ex:setpoint bldg:HotAisle_CO2_SP .
 bldg:HotAisle_CO2 a brick:CO2_Sensor ; ex:value 720 .
 bldg:HotAisle_CO2_SP a brick:CO2_Setpoint ; ex:value 700 .
