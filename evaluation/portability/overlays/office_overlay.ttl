@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix bldg:  <http://buildsys.org/ontologies/bldg6#> .
@prefix ex:    <https://example.org/ontobot#> .

# Minimal overlay to support portable queries on bldg6 as an Office
# - two floors with numbers
# - one AHU on the top floor
# - a few zones with CO2 points and setpoints

bldg:floor1 a brick:Floor ; ex:floorNumber 1 .
bldg:floor2 a brick:Floor ; ex:floorNumber 2 .

bldg:AHU_Top a brick:Air_Handling_Unit ; brick:isLocatedIn bldg:floor2 .

# Zones and CO2
bldg:ZoneA a brick:Zone ; ex:hasCO2 bldg:ZoneA_CO2 ; ex:setpoint bldg:ZoneA_CO2_SP .
 bldg:ZoneA_CO2 a brick:CO2_Sensor ; ex:value 650 .
 bldg:ZoneA_CO2_SP a brick:CO2_Setpoint ; ex:value 800 .

bldg:ZoneB a brick:Zone ; ex:hasCO2 bldg:ZoneB_CO2 ; ex:setpoint bldg:ZoneB_CO2_SP .
 bldg:ZoneB_CO2 a brick:CO2_Sensor ; ex:value 900 .
 bldg:ZoneB_CO2_SP a brick:CO2_Setpoint ; ex:value 800 .

bldg:ZoneC a brick:Zone ; ex:hasCO2 bldg:ZoneC_CO2 ; ex:setpoint bldg:ZoneC_CO2_SP .
 bldg:ZoneC_CO2 a brick:CO2_Sensor ; ex:value 500 .
 bldg:ZoneC_CO2_SP a brick:CO2_Setpoint ; ex:value 800 .
